{
  "expo": {
    "name": "safe-docs-mobile",
    "slug": "safe-docs-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "safedocsmobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.safedocs.mobile",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan documents.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to select and upload documents.",
        "NSPhotoLibraryAddUsageDescription": "This app needs permission to save scanned documents to your photo library."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.safedocs.mobile",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "react-native-document-scanner-plugin",
        {
          "cameraPermission": "We need camera access, so you can scan documents"
        }
      ]
    ],
    "extra": {
      "apiBaseUrl": "http://192.168.100.94:8080",
      "google": {
        "expoClientId": "69276793339-fs9po7b8iic1hjq18lp7maeo93k26djr.apps.googleusercontent.com",
        "GOOGLE_CLIENT_ID_IOS": "69276793339-10h8obs53e9ftlseef61qfm5bkdic5j2.apps.googleusercontent.com",
        "GOOGLE_CLIENT_ID_ANDROID": "69276793339-fs9po7b8iic1hjq18lp7maeo93k26djr.apps.googleusercontent.com",
        "webClientId": "69276793339-fs9po7b8iic1hjq18lp7maeo93k26djr.apps.googleusercontent.com"
      }
    },
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}